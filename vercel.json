{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "pair.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/pair", "dest": "/index.html" },
    { "src": "/pair", "dest": "/pair.html" },
    { "src": "/api/code", "dest": "/index.js" },
 { "src": "/api/code", "dest": "/pair.js" }
  ]
}
